; This file is for unifying the coding style for different editors and IDEs
; See editorconfig.org

; top-most EditorConfig file
root = true

[*]
indent_style = tab
indent_size = 8
end_of_line = lf
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = true

[*.{json,js,scss,rb,tf,vue}]
indent_style = space
indent_size = 2
